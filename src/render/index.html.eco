<!-- banner -->
<main id="banner">
	<div class="container">
		<div class="wrapper">
			<figure>
				<img src="img/laravel_logo.png" alt="Laraconf2016">
				<figcaption>
					<span><b>#</b>LARACONF<b>BRASIL</b></span>
					<time datetime="2016-08-10">08/10/2016</time>
				</figcaption>
				<h2>FALTAM <span class="day">07</span> DIAS</h2>
				<small>PARA O MAIOR EVENTO DE LARAVEL DO BRASIL</small>
			</figure>
			<a class="btn btn-border" href="#" title="GARANTA O SEU INGRESSO">GARANTA O SEU INGRESSO</a>
		</div>
	</div>
	<div class="scroll-down">
		<span class="arrow"></span>
	</div>
</main><!-- /banner -->


